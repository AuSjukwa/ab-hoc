{
    "name": "react-ab-hoc",
    "version": "0.0.2",
    "description": "A react hoc for A/B testing",
    "author": "AuSjukwa",
    "license": "MIT",
    "keywords": [
        "A/B",
        "ab",
        "react",
        "hoc"
    ],
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "build": "tsup",
        "tsc": "tsc",
        "test": "vitest",
        "test:watch": "vitest --watch",
        "release": "pnpm publish --access public --no-git-check"
    },
    "peerDependencies": {
        "react": ">=16.0.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.4.2",
        "@testing-library/react": "^14.1.2",
        "@testing-library/user-event": "^14.5.2",
        "@types/node": "^20.10.6",
        "@types/react": "^18.2.45",
        "react": "^18.2.0",
        "tsup": "^8.0.1"
    }
}
